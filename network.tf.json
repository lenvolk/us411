{
    "variable": [
        {
            "vnet_cidr_range": {
                "type": "map",
                "description": "CIDRS blocks per environment",
                "default": {
                "Development-main"     : "10.0.0.0/16",
                "Development-sec"      : "10.1.0.0/16",
                "Uat-main"             : "10.2.0.0/16",
                "Uat-sec"              : "10.3.0.0/16",
                "Production-main"      : "10.4.0.0/16",
                "Production-sec"       : "10.4.0.0/16"
                }
            }
        }
    ],
    "output": [
        {
            "vnet_cidr_range":
              { 
                "value": "${var.vnet_cidr_range [format(\"%s-%s\", var.env, var.net_topology)]}"
              }
        }
    ]
}
